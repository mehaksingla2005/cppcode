//missing element in an array
class Solution{
  public:
    int missingNumber(vector<int>& array, int n) {
        // Your code goes here
        int XOR1=0;
        int XOR2=0;
        for(int i=0;i<n;i++){
            XOR1 = XOR1^array[i];
            XOR2 = XOR2^(i+1);
        }
        int ans=XOR1^XOR2;
        return ans;
    }
};
