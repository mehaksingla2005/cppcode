#include <iostream>
using namespace std;

int main() {
    //using sum method to find missing element
    int arr[4]={1,2,4,5};
    int n=5;
    int sum=n*(n+1)/2;
    int sum1=0;
    for(int i=0;i<n-1;i++){
        sum1+=arr[i];
    }
    int ans =sum-sum1;
    cout<<"missing element is:"<<ans;
    
    

    return 0;
}
